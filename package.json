{
  "name": "@zodyac/zod-mongoose",
  "version": "4.1.0",
  "description": "A library that allows you to generate mongoose schemas from zod objects.",
  "scripts": {
    "test": "jest",
    "test:run": "node test.js",
    "test:badges": "jest --coverage && make-coverage-badge --report-path ./artifacts/coverage/coverage-summary.json --output-path ./badges/coverage.svg",
    "build": "tsup",
    "lint": "biome check",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "private": false,
  "sideEffects": true,
  "files": [
    "dist"
  ],
  "author": "bebrasmell",
  "license": "MIT",
  "homepage": "https://zodyac.dev/toolbox/zod-mongoose",
  "repository": {
    "type": "git",
    "url": "https://github.com/git-zodyac/mongoose"
  },
  "keywords": [
    "zod",
    "mongoose",
    "schema",
    "validation",
    "typescript",
    "type",
    "types",
    "type-safe",
    "validation",
    "mongodb",
    "mongo",
    "database",
    "db",
    "orm",
    "odm",
    "document"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/core": "^1.11.29",
    "@swc/jest": "^0.2.38",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "jest": "^29.7.0",
    "make-coverage-badge": "^1.2.0",
    "mongoose": "^8.15.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.42"
  },
  "peerDependencies": {
    "mongoose": "^8.0.3",
    "zod": "^3.22.4"
  },
  "packageManager": "pnpm@10.8.0"
}
